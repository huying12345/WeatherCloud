@{
    ViewBag.Title = "雷达卫星";
    Layout = "ShowLayout.cshtml";
}
<div class="show_content">
    <span id="show_time" class="show_time"></span>
    <div class="show_map">
        <img src="" id="imgshow" alt="暂无图片" />
    </div>
    <div class="show_btns">
        <span id="previousbutton" class="iconfont icon-prev"></span>
        <span id="playbutton" class="iconfont icon-play"></span>
        <span id="nextbutton" class="iconfont icon-next"></span>
    </div>

    <div id="Subscribe" class="Subscribe">
        <!--ko ifnot: issubscribe-->
        <span data-bind="click:addSubscribeNode">加入收藏</span>
        <!--/ko-->
        <span data-bind="visible:issubscribe,click:removeSubscribeNode">取消收藏</span>
    </div>
</div>
