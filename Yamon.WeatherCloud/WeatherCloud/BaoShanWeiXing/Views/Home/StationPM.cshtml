@{
    ViewBag.Title = "雷达卫星";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

@section styles{
    <link href="~/Static/ImgPlay/css/style.css" rel="stylesheet" />


    <style>
        .noselect {
            background-color: white;
            background: url(../../Static/ImgPlay/Images/noselect.png) repeat;
        }

        .select {
            color: #ffffff !important;
            background: url(../../Static/ImgPlay/Images/select.png) repeat;
            vertical-align: middle;
        }

            .select a {
                color: #ffffff !important;
            }

        #district {
            border-collapse: collapse;
        }

            #district td {
                height: 30px;
                line-height: 30px;
                text-align: center;
                padding: 3px;
            }

        #imgshow {
        }

        .imageinfo {
            padding: 0px;
            margin-top: 12px;
        }

            .imageinfo p {
                margin: 2px;
            }

        .headerdiv {
            height: 35px;
            background: #ffffff url('../../Static/ImgPlay/Images/head.png') repeat-x;
            font-weight: bold;
            padding: 10px 3px 5px 5px;
        }

        body {
            color: #555555;
        }
    </style>

}
@section scripts{
<script src="~/Static/echarts/echarts.min.js"></script>
    <script src="~/Static/ImgPlay/js/touchswipe.js"></script>
}

<div style="background-color: #ffffff;  ">

    <div style="width:100%; height:1px;"></div>


    <table class="navs" cellpadding="0" cellspacing="0" style=" height:30px;display:none" id="tablediv">

        <tr data-bind='foreach: lines3' class="line">
            <td class="selecttabicon col-xs-1" style=" vertical-align:middle;border:2px solid #DDDDDD" align="center" name="1"><a data-bind="text:NodeName,attr:{class:NodeID }"></a></td>



        </tr>

    </table>
    <div style="width:100%;height:300px;" id="world-map"></div>

</div>


